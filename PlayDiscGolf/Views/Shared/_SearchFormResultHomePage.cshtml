@using PlayDiscGolf.ViewModels.Home

@model List<SearchResultAjaxFormViewModel>
@{
    Layout = null;
}


@if (Model.Count > 0)
{
    <ul>
        @foreach (var item in Model)
        {
            <li>
                <button class="btn btn-secondary" id="@item.CourseID.ToString()">@item.Area - @item.FullName - @item.Holes</button>
            </li>
        }
    </ul>
}