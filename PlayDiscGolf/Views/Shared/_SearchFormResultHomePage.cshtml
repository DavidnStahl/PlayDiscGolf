@using PlayDiscGolf.ViewModels.Home

@model List<SearchResultAjaxFormViewModel>
@{
    Layout = null;
}


    @if (Model.Count > 0)
    {
        <ul class="list-unstyled">
            @foreach (var item in Model)
            {
                <li class="mb-3">
                    <a type="button" asp-action="Index" asp-controller="Course" asp-route-courseID="@item.CourseID.ToString()" class="btn btn-secondary" id="@item.CourseID.ToString()">@item.FullName</a>
                </li>
            }
        </ul>
    }


