@using PlayDiscGolf.ViewModels.Home

@model List<SearchResultAjaxFormViewModel>
@{
    Layout = null;
}


@if (Model.Count > 0)
{
    <ul>
        @foreach (var item in Model)
        {
            <li>
                <a type="button" asp-action="Index" asp-controller="Course" asp-route-courseID="@item.CourseID.ToString()" class="btn btn-secondary" id="@item.CourseID.ToString()">@item.Area - @item.FullName - @item.Holes</a>
            </li>
        }
    </ul>
}