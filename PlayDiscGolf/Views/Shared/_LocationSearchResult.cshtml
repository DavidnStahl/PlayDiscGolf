@model List<PlayDiscGolf.Models.ViewModels.SearchLocationItemViewModel>

@{
    Layout = null;
}




@if (Model.Count > 0)
{
    <ul>
        @foreach (var item in Model)
        {
            
            <li><a class="nav-link text-black-50" asp-area="" asp-controller="AdminCourse" asp-action="SelectedLocation" asp-route-id="@item.LocationID">@item.Name</a></li>
        }
    </ul>
}