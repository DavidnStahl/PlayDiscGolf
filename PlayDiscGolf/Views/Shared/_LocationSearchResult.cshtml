@model List<PlayDiscGolf.Models.ViewModels.SearchLocationItemViewModel>


@{
    Layout = null;
}




@if (Model.Count > 0)
{
    <ul>
        @foreach (var item in Model)
        {
            <li>
                <button class="locationButton" id="@item.LocationID">@item.Name</button>
            </li>



        }
    </ul>
}


<script src="~/js/site.js" asp-append-version="true"></script>
